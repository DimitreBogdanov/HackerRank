package com.application.main;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
        //elements
        int n = in.nextInt();
        //rotations
        int k = in.nextInt();
        //queries
        int q = in.nextInt();
        int rot=k%n;
        int[] a = new int[n];
        
        for(int i = 0; i < n; i++){
            a[i] = in.nextInt();
        }
        
        int[] results = new int[q]; 
        for(int i = 0; i < q; i++){
            //results[i] = in.nextInt();
            //System.out.println(a[(n-k+in.nextInt())%n]);
        	int num = in.nextInt();
        	if(num-(k%n) >= 0){
        		System.out.println(a[(n-(k%n))]);
        	}else{
        		System.out.println(a[num-(k%n)+a.length]);
        	}
        }
        
        //for(int i = 0; i < q; i++){
        	//System.out.println(a[(n-k+results[i])%n]);
        //}

	}

}
